	$(document).ready(function(){
		$('#createNewId').click(function(event){
			openModel();
		})
	});
	function openModel(){
		$("#myModal").dialog({
            modal: true,
            autoOpen: false,
            title: "jQuery Dialog",
            width: 300,
            height: 150
	});
	}
	
</script>


